{
  "navigation": {
    "HomeLogoLink": {
      "xpath": "//div[@class='logo']/a",
      "action": "click",
      "type": "link"
    },
    "AboutLink": {
      "xpath": "//a[@href='about.html']",
      "action": "click",
      "type": "link"
    },
    "ServicesLink": {
      "xpath": "//a[@href='services.html']",
      "action": "click",
      "type": "link"
    },
    "TestimonialsLink": {
      "xpath": "//a[@href='testimonials.html']",
      "action": "click",
      "type": "link"
    },
    "UploadLink": {
      "xpath": "//a[@href='upload.html']",
      "action": "click",
      "type": "link"
    },
    "ContactLink": {
      "xpath": "//a[@href='contact.html']",
      "action": "click",
      "type": "link"
    },
    "PrivacyLink": {
      "xpath": "//a[@href='privacy.html']",
      "action": "click",
      "type": "link"
    },
    "FAQLink": {
      "xpath": "//a[@href='faq.html']",
      "action": "click",
      "type": "link"
    },
    "ExploreServicesCTA": {
      "xpath": "//a[@class='cta' and @href='services.html']",
      "action": "click",
      "type": "link"
    },
    "MenuToggleButton": {
      "xpath": "//button[@class='menu-toggle']",
      "action": "click",
      "type": "button"
    }
  },
  "content": {
    "MainHeader": {
      "xpath": "//h1[text()='Trusted Expertise. Clinical Precision.']",
      "action": "assert",
      "type": "text"
    },
    "SubtitleParagraph": {
      "xpath": "//p[@class='subtitle']",
      "action": "assert",
      "type": "text"
    },
    "WhyChooseHeader": {
      "xpath": "//h2[text()='Why Attorneys Choose Catalyst']",
      "action": "assert",
      "type": "text"
    },
    "StrategicServicesHeader": {
      "xpath": "//h2[text()='Strategic Services']",
      "action": "assert",
      "type": "text"
    },
    "CredentialsList": {
      "xpath": "//section[h2[text()='Why Attorneys Choose Catalyst']]/ul",
      "action": "assert",
      "type": "list"
    },
    "ServicesList": {
      "xpath": "//section[h2[text()='Strategic Services']]/ul",
      "action": "assert",
      "type": "list"
    },
    "PrivacyPolicyLinkInList": {
      "xpath": "//section[h2[text()='Why Attorneys Choose Catalyst']]//a[@href='privacy.html']",
      "action": "click",
      "type": "link"
    }
  },
  "modal": {
    "ConfirmationModal": {
      "xpath": "//div[@id='confirmationModal']",
      "action": "assert",
      "type": "modal"
    },
    "ModalHeader": {
      "xpath": "//div[@class='modal-content']/h2",
      "action": "assert",
      "type": "text"
    },
    "ModalMessage": {
      "xpath": "//div[@class='modal-content']/p",
      "action": "assert",
      "type": "text"
    },
    "ModalConfirmButton": {
      "xpath": "//button[@id='confirmButton']",
      "action": "click",
      "type": "button"
    }
  },
  "layout": {
    "SidebarImage": {
      "xpath": "//aside[@class='sidebar-image']/img",
      "action": "assert",
      "type": "image"
    },
    "FooterText": {
      "xpath": "//footer/p",
      "action": "assert",
      "type": "text"
    }
  }
}